{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nom de fabrique de données",
			"defaultValue": "adf-salesinsightcuxm0611"
		},
		"LS_ADLS_SalesInsight_accountKey": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « accountKey » de « LS_ADLS_SalesInsight »"
		},
		"LS_ADLS_SalesInsight_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stsalesinsightcuxm0611.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI pour le compte de stockage contenant des modèles ARM liés"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Jeton SAS pour le compte de stockage contenant des modèles ARM liés"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-salesinsightcuxm0611_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_SalesInsight_accountKey": {
						"value": "[parameters('LS_ADLS_SalesInsight_accountKey')]"
					},
					"LS_ADLS_SalesInsight_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_SalesInsight_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}